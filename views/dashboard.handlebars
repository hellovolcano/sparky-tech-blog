<a href="/dashboard/new-post">Add new post</a>

{{#if posts.length }}
<section>
    <h2>Your Posts</h2>
    <div>
        {{#each posts as |post|}}
            {{> post-info post}}
            <a href="/dashboard/edit/{{post.id}}" class="edit-link">Edit post</a>
        {{/each}}
    </div>
</section>
{{/if}}